{
    "manifestVersion": 1,
    "id": "build-tools",
    "name": "Rocket Surgeons Guild - Variable Tools",
    "version": "0.0.1",
    "publisher": "RocketSurgeonsGuild",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Helpers for serializing and deserializing variables between build, jobs, and releases.",
    "categories": ["Build and release"],
    "files": [
        {
            "path": "tasks/UniqueString"
        },
        {
            "path": "tasks/DeserializeVariables"
        },
        {
